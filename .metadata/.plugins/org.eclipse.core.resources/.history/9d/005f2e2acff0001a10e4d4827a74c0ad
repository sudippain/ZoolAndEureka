package com.test.collection;

class M{
	public void run() {
		for(int i =0;i<5;i++)
			System.out.println("Thread from class A"+i);
	}
}

class N{
	public void run() {
		for(int i =0;i<5;i++)
			System.out.println("Thread from class N"+i);
	}
}

class O{
	public void run() {
		for(int i =0;i<5;i++)
			System.out.println("Thread from class O"+i);
	}
}



public class ThreadImpl {

	public void run() {
		for(int i =0;i<5;i++)
			System.out.println("Thread from class A"+i);
	}
}
