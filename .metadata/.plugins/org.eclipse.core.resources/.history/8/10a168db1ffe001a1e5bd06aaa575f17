package com.feature.StreamApi;



public class StringValueOf {
	
	static String change(int num) {
		String a = (String)(num);
		return a;
	}

	public static void main(String[] args) {
		int n = 3;
		String val = String.valueOf(3) ;
		System.out.println(val);
		String s = StringValueOf::change;
		
	}
}
