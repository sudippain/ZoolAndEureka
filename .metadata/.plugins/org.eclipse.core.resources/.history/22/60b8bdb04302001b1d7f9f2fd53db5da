package com.test.collection;

import static org.junit.Assert.*;

import java.util.Arrays;
import java.util.LinkedList;

import org.junit.Assert;
import org.junit.Test;

public class BNYMLinkedListTest {

	@Test
	public void test1() {
		
		LinkedList<Integer> l = new LinkedList<>(Arrays.asList(1,2));
		
		Integer[] arr = {2};
		Assert.assertArrayEquals(BNYMLinkedList.middleList(l).toArray(), arr);
	}
	
	@Test
	public void test2() {
		
		LinkedList<Integer> l = new LinkedList<>(Arrays.asList(1,2,3));
		
		Integer[] arr = {2};
		Assert.assertArrayEquals(BNYMLinkedList.middleList(l).toArray(), arr);
	}

}
