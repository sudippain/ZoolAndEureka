		ArrayList<String> l = new ArrayList<String>();
		
		l.add("Apple");
		l.add("Banana");
		l.add("Pear");
		l.add("Mango");
		
		
		System.out.println("Before Sorting");
		
		for(String i : l)
			System.out.println(i);
		Collections.sort(l);
		Collections.reverse(l);
		System.out.println("After Sorting");
		for(String i : l)
