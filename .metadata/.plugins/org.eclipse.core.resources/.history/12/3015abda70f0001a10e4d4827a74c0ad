package com.test.collection;

 class Z{
	 
	
	 int sum(int a,int b) {
		 return a+b;
	 }
	
}
public class overRide extends Z{
	public overRide() {
		super();
	}
	int sum(int a,int b) {
		 return b%a;
	 }
public static void main(String[] args) {
	overRide obj = new overRide();
	
	System.out.println(obj.sum(2, 3));
	
}
}
