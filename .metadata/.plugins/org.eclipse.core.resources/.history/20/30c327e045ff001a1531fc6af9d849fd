package com.feature.StreamApi;

public class RaA implements Runnable{
	public void run() {
		System.out.println("This is from RaA");
	}
}
public class RaB implements Runnable{
	public void run() {
		System.out.println("This is from RaA");
	}
}
public class RaC implements Runnable{
	public void run() {
		System.out.println("This is from RaA");
	}
}

public class Runnable {

	public static void main(String[] args) {
		
		Thread t1 = new Thread(new Rac());
	}
}
