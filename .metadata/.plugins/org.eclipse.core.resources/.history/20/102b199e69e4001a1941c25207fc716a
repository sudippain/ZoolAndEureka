package com.test.collection;




public class TestOverride {
	
	public void method1() {
		String a= "madam";
		char arr[] =a.toCharArray();
			
		int s=0;
			for(int i=0;i<arr.length;i++) {
				int index=arr.length-1;
				if(arr[i]==arr[index])
				{
					s++;
				}
			}
			if(s==arr.length) {
				System.out.println("Palindrome");
			}
			else {
				System.out.println("Not Palindrome");
			}
	}
	
}
